#prices
	width 100%
	padding 60px 0
.price_packet
	border-radius: 20px
	background-color #fff
	box-shadow: 0px 5px 11px 0px rgba(0,0,0,0.15);
	border: 1px solid #f4f1f3;
	margin: 0 auto 40px
	width 100%
	max-width 320px
	padding 0 20px
.price_packet_head
	padding 20px 0
	text-align: center
.price_packet_head_title
	font-size: 26px
	color #d95e7b
	font-family: "GilroyExtraBold", sans-serif
	line-height: 1.3
	small
		font-size: 70%
		display block
		color #000
		font-family: "GilroyLight", sans-serif
.price_packet_body
	padding 20px 0
	border-top: 1px solid rgba(#d95e7b, .35)
	border-bottom: 1px solid rgba(#d95e7b, .35)
	min-height 500px
.price_packet_body_title
	font-size: 20px
	margin-bottom: 15px
	font-family: "GilroyExtraBold", sans-serif
	line-height: 1.3
	text-align: center
	small
		font-size: 70%
		font-family: "GilroyLight", sans-serif
		display block
		padding-top: 5px
.price_packet_list
	padding-left: 10px
	li
		padding-left: 13px
		position relative
		margin-bottom: 10px
		list-style: none
		font-size: 16px
		&:before
			content: ''
			position: absolute
			top 9px
			left 0
			width: 7px
			height 7px
			border-radius: 50px
			background-color: #d95e7b
		&:last-child
			margin-bottom: 0
.price_packet_footer
	padding 20px 0
	text-align: center
.cost
	font-size: 24px
	margin-bottom: 20px
	font-family: "GilroyExtraBold", sans-serif
	line-height: 1.3
	span
		display block
.cost_full
	span
		text-decoration: line-through
.cost_sale
	color red
.cost_today
	color #00AC00
.cost_button
	margin-top: 30px
.prepayment
	text-align: center
.prepaymant_title
	font-size: 34px
	color red
	font-family: "GilroyExtraBold", sans-serif
	line-height: 1.3
.prepayment_text
	font-size: 24px
	margin 0 0 20px
.timer_title
	font-size: 30px
	text-align: center
	margin: 40px 0 0
	font-family: "GilroyExtraBold", sans-serif
	line-height: 1.3
.alert-danger
	color: #a94442;
	background-color: #f2dede;
	padding: 10px;
	width 100%
	max-width 360px
	margin 40px auto 0
	border: 1px solid #ebccd1;
	border-radius: 4px;
	font-size: 24px;
	font-family: 'OpenSans', sans-serif
	text-align: center
.flip-clock-wrapper
	width: 100%
	max-width 460px
	margin-top: 40px
.flip-clock-divider .flip-clock-label
	color #333
.flip-clock-dot
	background-color: #d95e7b
.flip-clock-wrapper ul li a div div.inn
	color: #fff
	background-color: #d95e7b
	text-shadow: 2px 2px 2px rgba(#022a37,.6);