#as_n_qs
	width 100%
	padding  230px 0 200px
	position relative
	height 100%
	.container
		position relative
		z-index 9
	&:before
		position: absolute;
		z-index: 0;
		content: none
		display: block;
		width: 100%;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		height: inherit;
		background-image: url("../img/as_n_qs.png");
		background-repeat: no-repeat;
		background-position: center;
		background-size: cover;
#as_n_qs_img
	position: absolute
	width: 100%
	top 0
	left 0
	height 100%
	margin 0 auto
.general_title_as_n_qs
	color #fff
	margin-bottom: 73px
	span
		color #d95e7b
.as_n_qs_container
	background-color: #fff
	width: 100%
	max-width: 558px;
	height: 127px;
	margin: 0 auto 29px
	border-radius: 63.5px
	padding 16px 20px 16px 141px
	background-position: 6px center
	background-repeat no-repeat
	color #22222f
	line-height: 1.3
.question_text
	margin-bottom: 9px
.answer_text
	font-family: "GilroyExtraBold", sans-serif;
