@font-face {
	font-family: 'GilroyExtraBold';
	src: url('../fonts/GilroyExtraBold.eot');
	src: url('../fonts/GilroyExtraBold.eot') format('embedded-opentype'),
		 url('../fonts/GilroyExtraBold.woff2') format('woff2'),
		 url('../fonts/GilroyExtraBold.woff') format('woff'),
		 url('../fonts/GilroyExtraBold.ttf') format('truetype'),
		 url('../fonts/GilroyExtraBold.svg#GilroyExtraBold') format('svg');
	font-display: fallback;
	font-weight: 900;
	font-style: normal;
}
@font-face {
	font-family: 'GilroyLight';
	src: url('../fonts/GilroyLight.eot');
	src: url('../fonts/GilroyLight.eot') format('embedded-opentype'),
		 url('../fonts/GilroyLight.woff2') format('woff2'),
		 url('../fonts/GilroyLight.woff') format('woff'),
		 url('../fonts/GilroyLight.ttf') format('truetype'),
		 url('../fonts/GilroyLight.svg#GilroyLight') format('svg');
	font-display: fallback;
	font-weight: 100;
	font-style: normal;
}
@font-face {
	font-family: 'OpenSans';
	src: url('../fonts/OpenSans.eot');
	src: url('../fonts/OpenSans.eot') format('embedded-opentype'),
		 url('../fonts/OpenSans.woff2') format('woff2'),
		 url('../fonts/OpenSans.woff') format('woff'),
		 url('../fonts/OpenSans.ttf') format('truetype'),
		 url('../fonts/OpenSans.svg#OpenSans') format('svg');
	font-display: fallback;
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'OpenSansLight';
	src: url('../fonts/OpenSansLight.eot');
	src: url('../fonts/OpenSansLight.eot') format('embedded-opentype'),
		 url('../fonts/OpenSansLight.woff2') format('woff2'),
		 url('../fonts/OpenSansLight.woff') format('woff'),
		 url('../fonts/OpenSansLight.ttf') format('truetype'),
		 url('../fonts/OpenSansLight.svg#OpenSansLight') format('svg');
	font-display: fallback;
	font-weight: 100;
	font-style: normal;
}

@import '../css/_css/bootstrap.min.css'

*
	margin 0
	padding 0
	outline none
body
	font-family: 'GilroyLight', sans-serif
	font-size: 16px
	background-color #fff
	background-image url(../img/body_bg.png)
	background-repeat repeat
	background-position: center
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 5px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.general_title
	font-size: 36px;
	font-family: "GilroyExtraBold", sans-serif;
	color: rgb(34, 34, 47);
	line-height: 1.423;
	text-align: center;
#main
	width: 100%
	padding 0 0 16px
	background-image url(../img/bonus_main_bg.png)
	background-repeat no-repeat
	background-position: center bottom
	background-size: cover
	.container
		position relative
		z-index 9
#header
	padding: 29px 0
	overflow hidden
	z-index: 9
	position relative
	margin-bottom: 5px
#logo
	float: left
	width 164px
	height auto
.contacts
	float: right
	text-align: right
	line-height  15px
	a
		font-size: 10px
		&:focus
			text-decoration: none
		.fa
			color: #d95e7b
			font-size: 15px
			margin: 0 10px 0 22px
			line-height: 15px
		.fa+span
			line-height: 15px
			font-size: 15px;
			font-family: "OpenSans", sans-serif;
			color: rgba(#fff, .8)
			text-align: right;
			&:hover
				text-decoration underline
.tel:first-child
	.fa-phone
		margin-left: 0
.pre
	font-size: 22px;
	font-family: "GilroyExtraBold", sans-serif;
	color: rgb(217, 94, 123);
	line-height: 1.2
	span
			font-family: "GilroyLight", sans-serif;
			color #fff
.main_title
	font-size: 38px;
	font-family: "GilroyExtraBold", sans-serif;
	color: rgb(254, 254, 254);
	line-height: 1.3;
	margin 10px 0
	width 100%
	max-width 660px
.main_date
	font-size: 30px
	font-family: "GilroyExtraBold", sans-serif;
	color:  #d95e7b
	text-decoration: underline
	margin 15px 0 
	span
		font-family: "GilroyLight", sans-serif;
		color: rgb(254, 254, 254);
		text-decoration: none
		display: inline-block
.main_video_wrapper
	max-width 456px
	margin 0
#feedback
	width 100%
	padding 60px 0
	min-height calc(100vh - 760px)
.general_title_bonus
	margin-bottom: 40px
	span
		color #d95e7b
#vk
#fb
	background-color #fff
	border-radius: 10px
	box-shadow: 0px 1px 11.83px 1.17px rgba(0, 0, 0, 0.1);
#footer
	width 100%
	padding 40px 0
	background-color: #020217
	text-align: center;
	h5
		font-size: 24px;
		color: #fff
		line-height: 1.042;
		margin 0
		font-family: 'GilroyExtraBold', sans-serif
	p
		font-size: 14px;
		font-family: "OpenSansLight", sans-serif;
		color: rgba(254, 254, 254, 0.6);
		line-height: 1.2;
.soc_buttons
	width 100%
	margin: 0 auto
	a
		display: inline-block
		margin: 20px 13px
		position relative
		width: 32px;
		height 32px
		line-height 34px
		background-color: #c73c5d
		border-radius: 50%
		font-size 18px
		.fa
			color #020217
		&:hover
			background-color: #020217
			box-shadow: 0 0 0 1px #c73c5d inset
			.fa
				color #c73c5d
@media screen and (min-width 2295px)
	#main
		padding-bottom: 60px
	#logo
		width: 200px
	.contacts a .fa
	.contacts a .fa+span
		font-size: 20px
		line-height: 20px
	#header
		margin-bottom: 40px
	.main_title
		margin 30px 0
	.main_video_wrapper
		margin-top: 40px
		max-width 650px
	#feedback
		width 100%
		padding 60px 0
		min-height calc(100vh - 1022px)
	.soc_buttons a
		width: 45px
		line-height: 45px
		height 45px
		font-size: 24px
	#footer p
		font-size: 18px
@media screen and (max-width 991px)
	#header
		padding 10px 0
	#logo
		float: none
		display block
		margin 0 auto
	.contacts
		float: none
		text-align: center
		a
			display: inline-block
			margin 5px 5px 0
			.fa
				margin 0 5px 0 10px
	#main
		text-align: center
	.main_title
		margin: 10px auto 20px
	.main_video_wrapper
		margin 0 auto
	#feedback
		padding 40px 0
	.general_title
		line-height: 1.3
	#footer
		padding 30px 0
@media screen and (max-width 767px)
	#vk
		margin-bottom: 25px
@media screen and (max-width 485px)
	#logo
		width: 140px
	.contacts a .fa
	.contacts a .fa + span
		font-size: 12px
		line-height: 12px
	.pre
		font-size: 20px
	.main_title
		font-size: 26px
		margin 5px auto 10px
	.main_date
		font-size: 22px
		margin 10px 0
	#feedback
		padding 30px 0
	.general_title
		font-size: 24px
		margin-bottom: 30px
	#footer
		padding 20px 0
		h5
			font-size: 20px
		p
			font-size: 12px
	.soc_buttons a
		width 30px
		height 30px
		line-height: 32px
		margin 10px 8px
@media screen and (max-width 380px)
	.pre
		font-size: 18px
	.main_title
		font-size: 24px
		line-height: 1.2
	.main_date
		font-size: 20px
	.general_title
		font-size: 22px
	#footer h5
		font-size: 18px